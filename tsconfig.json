{
  "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "strictNullChecks": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "outDir": "dist",
        "suppressImplicitAnyIndexErrors": true
  },
  "compileOnSave": false,
  "include": [
    "lib/**/*.ts"
  ]
}